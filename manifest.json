{
	"manifest_version": 2,
	"name": "WhiteBox",
	"version": "2.0.6",
	"author": "Joshua Bird",
	"browser_action": {
		"default_popup": "main.html",
		"default_icon": "chat-icon.png",
		"default_title": "WhiteBox"
	},
	"permissions": [
		"cookies",
		"storage",
		"activeTab",
		"system.display"
	],
	"optional_permissions": [
		"https://www.jblrd.com/"
	],
	"background": {
	    "scripts": [
	    	"background.js"
	    ]
	}
}